#!/bin/sh

set -e

# postrm - finish removal of the 'kdevelop'-package

GLOBAL_KDEVDOCTREEVIEW_DIR="/var/lib/kdevelop";
GLOBAL_HTDIG_DIR="$GLOBAL_KDEVDOCTREEVIEW_DIR/helpindex"

if [ "$1" = "purge" ]; then
    rm -rf "$GLOBAL_HTDIG_DIR"
    rm -rf "$GLOBAL_KDEVDOCTREEVIEW_DIR"
fi

#DEBHELPER#

exit 0

