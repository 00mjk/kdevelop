    <text>Configure &amp;Printer</text>
    <Action name="options_configure_printer_enscript" />
   </Menu>
   <Separator/>
   <Action name="options_kdevelop_setup" />
   <Action name="options_configure_toolbars" />
  </Menu>
  <Menu name="window" >
   <text>&amp;Window</text>
   <Merge/>
  </Menu>
  <Menu name="bookmarks" >
   <text>Book&amp;marks</text>
   <Action name="bookmarks_toggle" />
   <Action name="bookmarks_next" />
   <Action name="bookmarks_previous" />
   <Action name="bookmarks_clear" />
  </Menu>
  <Menu name="help" >
   <text>&amp;Help</text>
   <Merge/>
   <Action name="help_back" />
   <Action name="help_forward" />
   <Separator/>
   <Action name="help_search_marked_text" />
   <Action name="help_search_for_help_on" />
   <Action name="help_whats_this" />
   <Separator/>
   <Action name="help_welcome" />
   <Action name="help_user_manual" />
   <Action name="help_programming" />
   <Action name="help_tutorial" />
   <Action name="help_library_reference" />
   <Action name="help_c_reference" />
   <Separator/>
   <Action name="help_tip_of_the_day" />
   <Action name="help_kdevelop_homepage" />
   <Action name="help_report_bug" />
   <Separator/>
   <Action name="help_about_app" />
   <Action name="help_about_kde" />
  </Menu>
 </MenuBar>
 <ToolBar position="Top" iconText="IconOnly" noMerge="1" name="mainToolBar" fullWidth="false" >
  <Action name="project_open" />
  <Separator/>
  <Action name="file_open" />
  <Action name="file_save" />
  <Action name="file_print" />
  <Separator/>
  <Action name="edit_cut" />
  <Action name="edit_copy" />
  <Action name="edit_paste" />
  <Separator/>
  <Action name="build_compile_file" />
  <Action name="build_build" />
  <Action name="build_rebuild_all" />
  <Action name="build_debug_delay" />
  <Action name="build_run" />
  <Action name="stop_everything" />
  <Separator/>
  <Action name="view_treeview" />
  <Action name="view_output_view" />
 </ToolBar>
 <ToolBar name="debuggerToolBar" fullWidth="false" >
  <Action name="debug_run" />
  <Action name="debug_run_to_cursor" />
  <Action name="debug_step_over" />
  <Action name="debug_step_over_instr" />
  <Action name="debug_step_into" />
  <Action name="debug_step_into_instr" />
  <Action name="debug_step_out" />
  <Action name="debug_interrupt" />
  <Action name="debug_stop" />
 </ToolBar>
 <ToolBar name="browserToolBar" fullWidth="false" >
  <Merge/>
  <Separator/>
  <Action name="help_back" />
  <Action name="help_forward" />
  <Action name="help_stop" />
  <Action name="help_reload" />
  <Action name="help_user_manual" />
  <Separator/>
  <Action name="help_search_marked_text" />
  <Action name="help_search_for_help_on" />
  <Separator/>
  <Action name="help_whats_this" />
 </ToolBar>
</kpartgui>
