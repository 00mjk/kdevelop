   <Action name="build_execute" />
   <Action name="build_execute_with_arguments" />
   <Separator/>
   <Action name="build_distclean" />
   <Action name="build_autoconf_automake" />
   <Action name="build_configure" />
  </Menu>
  <Menu name="debug" >
   <text>&amp;Debug</text>
   <Merge/>
  </Menu>
  <Menu name="settings" >
