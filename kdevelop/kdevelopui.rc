<!DOCTYPE kpartgui >
<kpartgui version="2" name="kdevelop" >
 <MenuBar>
  <Menu name="file" >
   <text>&amp;File</text>
   <Merge/>
  </Menu>
  <Menu name="edit" >
   <text>&amp;Edit</text>
   <Merge/>
   <Separator/>
   <Action name="edit_select_all" />
   <Action name="edit_deselect_all" />
   <Action name="edit_invert_selection" />
  </Menu>
  <Menu name="view" >
   <text>&amp;View</text>
   <Merge/>
   <Separator/>
   <Action name="view_treeview" />
   <Action name="view_output_view" />
   <Separator/>
   <Action name="view_toolbar" />
   <Action name="view_browser_toolbar" />
   <Action name="view_taskbar" />
   <Action name="view_statusbar" />
   <Separator/>
   <Menu name="view_debug" >
    <text>&amp;Debugger Views...</text>
    <Action name="view_debug_variables" />
    <Action name="view_debug_breakpoints" />
    <Action name="view_debug_framestack" />
    <Action name="view_debug_disassemble" />
    <Action name="view_debug_debugger" />
   </Menu>
   <Separator/>
   <Action name="view_refresh" />
  </Menu>
  <Menu name="project" >
   <text>&amp;Project</text>
   <Action name="project_new" />
   <Action name="project_open" />
   <Action name="project_open_recent" />
   <Action name="project_close" />
   <Separator/>
   <Action name="project_new_class" />
   <Action name="project_add_existing_files" />
   <Action name="project_add_translation" />
   <Action name="project_file_properties" />
   <Separator/>
   <Action name="project_make_messages" />
   <Action name="project_make_api_doc" />
   <Action name="project_make_user_doc" />
   <Menu name="project_distribution" >
    <text>Make D&amp;istribution</text>
    <Action name="project_dist_targz" />
   </Menu>
   <Separator/>
   <Action name="project_options" />
  </Menu>
  <Menu name="build" >
   <text>&amp;Build</text>
   <Action name="build_compile_file" />
   <Action name="build_build" />
   <Action name="build_rebuild" />
   <Action name="build_clean_rebuild_all" />
   <Separator/>
   <Action name="build_stop" />
