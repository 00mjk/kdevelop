<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE kcfg SYSTEM "http://www.kde.org/standards/kcfg/1.0/kcfg.dtd">
<kcfg>
  <kcfgfile name="kdeveloprc"/>
  <group name="Code Completion">
    <entry name="automaticInvocation" key="Automatic Invocation" type="Bool">
    <default>false</default>
</entry>
    <entry name="minimalAutomaticCompletion" key="minimalAutomaticCompletion" type="Bool">
    <default>false</default>
</entry>
    <entry name="alwaysFullCompletion" key="alwaysFullCompletion" type="Bool">
    <default>true</default>
</entry>
    <entry name="alwaysMinimalCompletion" key="alwaysMinimalCompletion" type="Bool">
    <default>false</default>
    </entry>
  </group>
</kcfg>
