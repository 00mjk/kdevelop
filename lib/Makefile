# Tell versions [3.59,3.63) of GNU make to not export all variables.
# Otherwise a system limit (for SysV at least) may be exceeded.
.NOEXPORT:

#>+ 8
libkdevelop.la.closure: $(libkdevelop_la_OBJECTS) $(libkdevelop_la_DEPENDENCIES)
	@echo "int main() {return 0;}" > libkdevelop_la_closure.cpp
	@$(LTCXXCOMPILE) -c libkdevelop_la_closure.cpp
	@$(CXXLINK) libkdevelop_la_closure.lo $(libkdevelop_la_LDFLAGS) $(libkdevelop_la_OBJECTS) $(libkdevelop_la_LIBADD) $(LIBS)
	@rm -f libkdevelop_la_closure.* libkdevelop.la.closure
	@echo "timestamp" > libkdevelop.la.closure


