#>+ 5
force-reedit:
		cd $(top_srcdir) && $(AUTOMAKE) --gnu lib/Makefile
	cd $(top_srcdir) && perl admin/am_edit lib/Makefile.in


#>+ 11
libkdevelop_la.all_cpp.cpp: $(srcdir)/Makefile.in $(srcdir)/dummy.cpp  
	@echo 'creating libkdevelop_la.all_cpp.cpp ...'; \
	rm -f libkdevelop_la.all_cpp.files libkdevelop_la.all_cpp.final; \
	echo "#define KDE_USE_FINAL 1" >> libkdevelop_la.all_cpp.final; \
	for file in dummy.cpp ; do \
	  echo "#include \"$$file\"" >> libkdevelop_la.all_cpp.files; \
	  test ! -f $(srcdir)/$$file || egrep '^#pragma +implementation' $(srcdir)/$$file >> libkdevelop_la.all_cpp.final; \
	done; \
	cat libkdevelop_la.all_cpp.final libkdevelop_la.all_cpp.files  > libkdevelop_la.all_cpp.cpp; \
	rm -f libkdevelop_la.all_cpp.final libkdevelop_la.all_cpp.files

#>+ 3
clean-final:
	-rm -f libkdevelop_la.all_cpp.cpp

