#>+ 5
force-reedit:
		cd $(top_srcdir) && $(AUTOMAKE) --gnu lib/interfaces/Makefile
	cd $(top_srcdir) && perl admin/am_edit lib/interfaces/Makefile.in


#>+ 10
libkdevinterfaces_la_meta_unload.cpp:  kdevappfrontend.moc kdevlanguagesupport.moc kdevproject.moc kdevcompileroptions.moc kdevfactory.moc.cpp kdevcore.moc kdevmakefrontend.moc kdevversioncontrol.moc kdevpart.moc
	@echo 'creating libkdevinterfaces_la_meta_unload.cpp'
	-rm -f libkdevinterfaces_la_meta_unload.cpp
	@echo 'static const char * _metalist_libkdevinterfaces_la[] = {' > libkdevinterfaces_la_meta_unload.cpp
	cat  kdevappfrontend.moc kdevlanguagesupport.moc kdevproject.moc kdevcompileroptions.moc kdevfactory.moc.cpp kdevcore.moc kdevmakefrontend.moc kdevversioncontrol.moc kdevpart.moc | grep 'char.*className' | sed -e 's/.*[^A-Za-z0-9_:]\([A-Za-z0-9_:]*\)::className.*$$/\"\1\",/' | sort | uniq >> libkdevinterfaces_la_meta_unload.cpp
	@echo '0};' >> libkdevinterfaces_la_meta_unload.cpp
	@echo '#include <kunload.h>' >> libkdevinterfaces_la_meta_unload.cpp
	@echo '_UNLOAD(libkdevinterfaces_la)' >> libkdevinterfaces_la_meta_unload.cpp


#>+ 3
clean-moc-classes:
	-rm -f  libkdevinterfaces_la_meta_unload.cpp

#>+ 11
libkdevinterfaces_la.all_cpp.cpp: $(srcdir)/Makefile.in $(srcdir)/kdevfactory.cpp $(srcdir)/kdevpart.cpp $(srcdir)/kdevcore.cpp $(srcdir)/kdevproject.cpp $(srcdir)/kdevlanguagesupport.cpp $(srcdir)/kdevversioncontrol.cpp $(srcdir)/kdevmakefrontend.cpp $(srcdir)/kdevappfrontend.cpp $(srcdir)/kdevcompileroptions.cpp $(srcdir)/KDevMakeFrontendIface.cpp $(srcdir)/KDevAppFrontendIface.cpp $(srcdir)/KDevMakeFrontendIface_skel.cpp $(srcdir)/KDevAppFrontendIface_skel.cpp $(srcdir)/libkdevinterfaces_la_meta_unload.cpp  kdevfactory.moc.cpp kdevappfrontend.moc kdevmakefrontend.moc kdevversioncontrol.moc kdevpart.moc kdevlanguagesupport.moc kdevcompileroptions.moc kdevproject.moc kdevcore.moc 
	@echo 'creating libkdevinterfaces_la.all_cpp.cpp ...'; \
	rm -f libkdevinterfaces_la.all_cpp.files libkdevinterfaces_la.all_cpp.final; \
	echo "#define KDE_USE_FINAL 1" >> libkdevinterfaces_la.all_cpp.final; \
	for file in kdevfactory.cpp kdevpart.cpp kdevcore.cpp kdevproject.cpp kdevlanguagesupport.cpp kdevversioncontrol.cpp kdevmakefrontend.cpp kdevappfrontend.cpp kdevcompileroptions.cpp KDevMakeFrontendIface.cpp KDevAppFrontendIface.cpp KDevMakeFrontendIface_skel.cpp KDevAppFrontendIface_skel.cpp libkdevinterfaces_la_meta_unload.cpp  kdevfactory.moc.cpp; do \
	  echo "#include \"$$file\"" >> libkdevinterfaces_la.all_cpp.files; \
	  test ! -f $(srcdir)/$$file || egrep '^#pragma +implementation' $(srcdir)/$$file >> libkdevinterfaces_la.all_cpp.final; \
	done; \
	cat libkdevinterfaces_la.all_cpp.final libkdevinterfaces_la.all_cpp.files  > libkdevinterfaces_la.all_cpp.cpp; \
	rm -f libkdevinterfaces_la.all_cpp.final libkdevinterfaces_la.all_cpp.files

#>+ 3
clean-final:
	-rm -f libkdevinterfaces_la.all_cpp.cpp

