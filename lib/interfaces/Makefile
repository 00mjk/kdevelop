install-servicetypeDATA: $(servicetype_DATA)
	@$(NORMAL_INSTALL)
	$(mkinstalldirs) $(DESTDIR)$(servicetypedir)
	@list='$(servicetype_DATA)'; for p in $$list; do \
	  if test -f $(srcdir)/$$p; then \
	    echo " $(INSTALL_DATA) $(srcdir)/$$p $(DESTDIR)$(servicetypedir)/$$p"; \
	    $(INSTALL_DATA) $(srcdir)/$$p $(DESTDIR)$(servicetypedir)/$$p; \
	  else if test -f $$p; then \
	    echo " $(INSTALL_DATA) $$p $(DESTDIR)$(servicetypedir)/$$p"; \
	    $(INSTALL_DATA) $$p $(DESTDIR)$(servicetypedir)/$$p; \
	  fi; fi; \
	done

uninstall-servicetypeDATA:
	@$(NORMAL_UNINSTALL)
	list='$(servicetype_DATA)'; for p in $$list; do \
	  rm -f $(DESTDIR)$(servicetypedir)/$$p; \
	done

install-kdevelopincludeHEADERS: $(kdevelopinclude_HEADERS)
	@$(NORMAL_INSTALL)
	$(mkinstalldirs) $(DESTDIR)$(kdevelopincludedir)
	@list='$(kdevelopinclude_HEADERS)'; for p in $$list; do \
	  if test -f "$$p"; then d= ; else d="$(srcdir)/"; fi; \
	  echo " $(INSTALL_DATA) $$d$$p $(DESTDIR)$(kdevelopincludedir)/$$p"; \
	  $(INSTALL_DATA) $$d$$p $(DESTDIR)$(kdevelopincludedir)/$$p; \
	done

uninstall-kdevelopincludeHEADERS:
	@$(NORMAL_UNINSTALL)
	list='$(kdevelopinclude_HEADERS)'; for p in $$list; do \
	  rm -f $(DESTDIR)$(kdevelopincludedir)/$$p; \
	done

tags: TAGS

ID: $(HEADERS) $(SOURCES) $(LISP)
	list='$(SOURCES) $(HEADERS)'; \
	unique=`for i in $$list; do echo $$i; done | \
	  awk '    { files[$$0] = 1; } \
	       END { for (i in files) print i; }'`; \
	here=`pwd` && cd $(srcdir) \
	  && mkid -f$$here/ID $$unique $(LISP)

TAGS:  $(HEADERS) $(SOURCES)  $(TAGS_DEPENDENCIES) $(LISP)
	tags=; \
	here=`pwd`; \
	list='$(SOURCES) $(HEADERS)'; \
	unique=`for i in $$list; do echo $$i; done | \
	  awk '    { files[$$0] = 1; } \
	       END { for (i in files) print i; }'`; \
	test -z "$(ETAGS_ARGS)$$unique$(LISP)$$tags" \
	  || (cd $(srcdir) && etags $(ETAGS_ARGS) $$tags  $$unique $(LISP) -o $$here/TAGS)

