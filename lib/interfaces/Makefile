distclean-am:  distclean-noinstLTLIBRARIES distclean-compile \
		distclean-libtool distclean-tags distclean-depend \
		distclean-generic clean-am
	-rm -f libtool

distclean: distclean-am

maintainer-clean-am:  maintainer-clean-noinstLTLIBRARIES \
		maintainer-clean-compile maintainer-clean-libtool \
		maintainer-clean-tags maintainer-clean-depend \
		maintainer-clean-generic distclean-am
	@echo "This command is intended for maintainers to use;"
	@echo "it deletes files that may require special tools to rebuild."

maintainer-clean: maintainer-clean-am

.PHONY: mostlyclean-noinstLTLIBRARIES distclean-noinstLTLIBRARIES \
clean-noinstLTLIBRARIES maintainer-clean-noinstLTLIBRARIES \
mostlyclean-compile distclean-compile clean-compile \
maintainer-clean-compile mostlyclean-libtool distclean-libtool \
clean-libtool maintainer-clean-libtool uninstall-servicetypeDATA \
install-servicetypeDATA uninstall-kdevelopincludeHEADERS \
install-kdevelopincludeHEADERS tags mostlyclean-tags distclean-tags \
clean-tags maintainer-clean-tags distdir mostlyclean-depend \
distclean-depend clean-depend maintainer-clean-depend info-am info \
dvi-am dvi check check-am installcheck-am installcheck install-exec-am \
install-exec install-data-am install-data install-am install \
uninstall-am uninstall all-redirect all-am all installdirs \
mostlyclean-generic distclean-generic clean-generic \
maintainer-clean-generic clean mostlyclean distclean maintainer-clean


# Tell versions [3.59,3.63) of GNU make to not export all variables.
# Otherwise a system limit (for SysV at least) may be exceeded.
.NOEXPORT:

#>+ 9
KDevMakeFrontendIface.kidl: $(srcdir)/KDevMakeFrontendIface.h $(DCOPIDL_DEPENDENCIES)
	$(DCOPIDL) $(srcdir)/KDevMakeFrontendIface.h > KDevMakeFrontendIface.kidl || ( rm -f KDevMakeFrontendIface.kidl ; /bin/false )
KDevMakeFrontendIface_skel.cpp: KDevMakeFrontendIface.kidl
	$(DCOPIDL2CPP) --c++-suffix cpp --no-stub KDevMakeFrontendIface.kidl
KDevAppFrontendIface.kidl: $(srcdir)/KDevAppFrontendIface.h $(DCOPIDL_DEPENDENCIES)
	$(DCOPIDL) $(srcdir)/KDevAppFrontendIface.h > KDevAppFrontendIface.kidl || ( rm -f KDevAppFrontendIface.kidl ; /bin/false )
KDevAppFrontendIface_skel.cpp: KDevAppFrontendIface.kidl
	$(DCOPIDL2CPP) --c++-suffix cpp --no-stub KDevAppFrontendIface.kidl

