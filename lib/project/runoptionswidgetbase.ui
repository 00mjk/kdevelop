</widget>
<connections>
    <connection>
        <sender>addvar_button</sender>
        <signal>clicked()</signal>
        <receiver>run_options_widget</receiver>
        <slot>addVarClicked()</slot>
    </connection>
    <connection>
        <sender>removevar_button</sender>
        <signal>clicked()</signal>
        <receiver>run_options_widget</receiver>
        <slot>removeVarClicked()</slot>
    </connection>
    <connection>
        <sender>browseMainButton</sender>
        <signal>clicked()</signal>
        <receiver>run_options_widget</receiver>
        <slot>browseMainProgram()</slot>
    </connection>
</connections>
<tabstops>
    <tabstop>mainprogram_edit</tabstop>
    <tabstop>progargs_edit</tabstop>
    <tabstop>listview</tabstop>
    <tabstop>addvar_button</tabstop>
    <tabstop>removevar_button</tabstop>
    <tabstop>startinterminal_box</tabstop>
</tabstops>
<slots>
    <slot access="protected">addVarClicked()</slot>
    <slot>browseMainProgram()</slot>
    <slot access="protected">removeVarClicked()</slot>
</slots>
<layoutdefaults spacing="6" margin="11"/>
</UI>
