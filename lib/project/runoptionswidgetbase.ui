<!DOCTYPE UI><UI version="3.1" stdsetdef="1">
<class>RunOptionsWidgetBase</class>
<widget class="QWidget">
    <property name="name">
        <cstring>run_options_widget</cstring>
    </property>
    <property name="geometry">
        <rect>
            <x>0</x>
            <y>0</y>
            <width>625</width>
            <height>449</height>
        </rect>
    </property>
    <property name="caption">
        <string>Run Options</string>
    </property>
    <vbox>
        <property name="name">
            <cstring>unnamed</cstring>
        </property>
        <property name="margin">
            <number>11</number>
        </property>
        <property name="spacing">
            <number>6</number>
        </property>
        <widget class="QGroupBox">
            <property name="name">
                <cstring>GroupBox1</cstring>
            </property>
            <property name="frameShape">
                <enum>Box</enum>
            </property>
            <property name="frameShadow">
                <enum>Sunken</enum>
            </property>
            <property name="title">
                <string>Run</string>
            </property>
            <grid>
                <property name="name">
                    <cstring>unnamed</cstring>
                </property>
                <property name="margin">
                    <number>11</number>
                </property>
                <property name="spacing">
                    <number>6</number>
                </property>
                <widget class="QLabel" row="2" column="0" rowspan="1" colspan="2">
                    <property name="name">
                        <cstring>progargs_label_3</cstring>
                    </property>
                    <property name="text">
                        <string>Program arguments:</string>
                    </property>
                </widget>
                <widget class="QLineEdit" row="3" column="0" rowspan="1" colspan="2">
                    <property name="name">
                        <cstring>progargs_edit</cstring>
                    </property>
                </widget>
                <widget class="QLineEdit" row="1" column="0">
                    <property name="name">
                        <cstring>mainprogram_edit</cstring>
                    </property>
                </widget>
                <widget class="QPushButton" row="1" column="1">
                    <property name="name">
                        <cstring>browseMainButton</cstring>
                    </property>
                    <property name="text">
                        <string>Browse...</string>
                    </property>
                </widget>
                <widget class="QLabel" row="0" column="0" rowspan="1" colspan="2">
                    <property name="name">
                        <cstring>mainprogram_label_3</cstring>
                    </property>
                    <property name="text">
                        <string>Main program (relative to build directory):</string>
                    </property>
                </widget>
            </grid>
        </widget>
        <widget class="QGroupBox">
            <property name="name">
                <cstring>env_var_group</cstring>
            </property>
            <property name="title">
                <string>Environment Variables</string>
            </property>
        </widget>
        <widget class="QLayoutWidget">
            <property name="name">
                <cstring>layout1</cstring>
            </property>
            <hbox>
                <property name="name">
                    <cstring>unnamed</cstring>
                </property>
                <widget class="QCheckBox">
                    <property name="name">
                        <cstring>startinterminal_box</cstring>
                    </property>
                    <property name="text">
                        <string>Start in external terminal</string>
                    </property>
                </widget>
                <widget class="QCheckBox">
                    <property name="name">
                        <cstring>autocompile_box</cstring>
                    </property>
                    <property name="text">
                        <string>Automatically compile before execution</string>
                    </property>
                </widget>
            </hbox>
        </widget>
    </vbox>
</widget>
<connections>
    <connection>
        <sender>browseMainButton</sender>
        <signal>clicked()</signal>
        <receiver>run_options_widget</receiver>
        <slot>browseMainProgram()</slot>
    </connection>
</connections>
<tabstops>
    <tabstop>mainprogram_edit</tabstop>
    <tabstop>progargs_edit</tabstop>
    <tabstop>startinterminal_box</tabstop>
</tabstops>
<slots>
    <slot>browseMainProgram()</slot>
</slots>
<layoutdefaults spacing="6" margin="11"/>
</UI>
