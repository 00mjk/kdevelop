                <widget class="QLabel" row="0" column="0" rowspan="1" colspan="2">
                    <property name="name">
                        <cstring>mainprogram_label_3</cstring>
                    </property>
                    <property name="text">
                        <string>Main program (relative to project directory):</string>
                    </property>
                </widget>
            </grid>
        </widget>
        <widget class="QGroupBox">
            <property name="name">
                <cstring>GroupBox2</cstring>
            </property>
            <property name="title">
                <string>Environment Variables</string>
            </property>
