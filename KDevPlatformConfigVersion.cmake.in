set( PACKAGE_VERSION "@KDEVPLATFORM_VERSION_MAJOR@.@KDEVPLATFORM_VERSION_MINOR@.@KDEVPLATFORM_VERSION_PATCH@" )
if( PACKAGE_FIND_VERSION_MAJOR EQUAL @KDEVPLATFORM_VERSION_MAJOR@ AND PACKAGE_FIND_VERSION_MINOR EQUAL @KDEVPLATFORM_VERSION_MINOR@ AND PACKAGE_FIND_VERSION_PATCH EQUAL @KDEVPLATFORM_VERSION_PATCH@ )

    set( PACKAGE_VERSION_COMPATIBLE true )
    set( PACKAGE_VERSION_EXACT true )

else( PACKAGE_FIND_VERSION_MAJOR EQUAL @KDEVPLATFORM_VERSION_MAJOR@ AND PACKAGE_FIND_VERSION_MINOR EQUAL @KDEVPLATFORM_VERSION_MINOR@ AND PACKAGE_FIND_VERSION_PATCH EQUAL @KDEVPLATFORM_VERSION_PATCH@ )

    set( PACKAGE_VERSION_EXACT false )

    if( @KDEVPLATFORM_VERSION_MAJOR@ LESS PACKAGE_FIND_VERSION_MAJOR )

        set( PACKAGE_VERSION_COMPATIBLE false )

    else( @KDEVPLATFORM_VERSION_MAJOR@ LESS PACKAGE_FIND_VERSION_MAJOR )
        if( @KDEVPLATFORM_VERSION_MINOR@ LESS PACKAGE_FIND_VERSION_MINOR )

            set( PACKAGE_VERSION_COMPATIBLE false )

        else( @KDEVPLATFORM_VERSION_MINOR@ LESS PACKAGE_FIND_VERSION_MINOR )
            if( @KDEVPLATFORM_VERSION_PATCH@ LESS PACKAGE_FIND_VERSION_PATCH )

                set( PACKAGE_VERSION_COMPATIBLE false )

            else( @KDEVPLATFORM_VERSION_PATCH@ LESS PACKAGE_FIND_VERSION_PATCH )

                set( PACKAGE_VERSION_COMPATIBLE true )

            endif( @KDEVPLATFORM_VERSION_PATCH@ LESS PACKAGE_FIND_VERSION_PATCH )
        endif( @KDEVPLATFORM_VERSION_MINOR@ LESS PACKAGE_FIND_VERSION_MINOR )
    endif( @KDEVPLATFORM_VERSION_MAJOR@ LESS PACKAGE_FIND_VERSION_MAJOR )

endif( PACKAGE_FIND_VERSION_MAJOR EQUAL @KDEVPLATFORM_VERSION_MAJOR@ AND PACKAGE_FIND_VERSION_MINOR EQUAL @KDEVPLATFORM_VERSION_MINOR@ AND PACKAGE_FIND_VERSION_PATCH EQUAL @KDEVPLATFORM_VERSION_PATCH@ )


