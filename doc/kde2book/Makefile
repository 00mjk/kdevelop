install-indexDATA: $(index_DATA)
	@$(NORMAL_INSTALL)
	$(mkinstalldirs) $(DESTDIR)$(indexdir)
	@list='$(index_DATA)'; for p in $$list; do \
	  if test -f $(srcdir)/$$p; then \
	    echo " $(INSTALL_DATA) $(srcdir)/$$p $(DESTDIR)$(indexdir)/$$p"; \
	    $(INSTALL_DATA) $(srcdir)/$$p $(DESTDIR)$(indexdir)/$$p; \
	  else if test -f $$p; then \
	    echo " $(INSTALL_DATA) $$p $(DESTDIR)$(indexdir)/$$p"; \
	    $(INSTALL_DATA) $$p $(DESTDIR)$(indexdir)/$$p; \
	  fi; fi; \
	done

uninstall-indexDATA:
	@$(NORMAL_UNINSTALL)
	list='$(index_DATA)'; for p in $$list; do \
	  rm -f $(DESTDIR)$(indexdir)/$$p; \
	done
