#>+ 8
libkdevgrepview.la.closure: $(libkdevgrepview_la_OBJECTS) $(libkdevgrepview_la_DEPENDENCIES)
	@echo "int main() {return 0;}" > libkdevgrepview_la_closure.cpp
	@$(LTCXXCOMPILE) -c libkdevgrepview_la_closure.cpp
	@$(CXXLINK) libkdevgrepview_la_closure.lo $(libkdevgrepview_la_LDFLAGS) $(libkdevgrepview_la_OBJECTS) $(libkdevgrepview_la_LIBADD) $(LIBS)
	@rm -f libkdevgrepview_la_closure.* libkdevgrepview.la.closure
	@echo "timestamp" > libkdevgrepview.la.closure


