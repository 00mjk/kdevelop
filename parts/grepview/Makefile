#>+ 10
libkdevgrepview_la_meta_unload.cpp:  grepdlg.moc grepviewpart.moc grepviewfactory.moc grepconfigwidget.moc grepviewwidget.moc
	@echo 'creating libkdevgrepview_la_meta_unload.cpp'
	-rm -f libkdevgrepview_la_meta_unload.cpp
	@echo 'static const char * _metalist_libkdevgrepview_la[] = {' > libkdevgrepview_la_meta_unload.cpp
	cat  grepdlg.moc grepviewpart.moc grepviewfactory.moc grepconfigwidget.moc grepviewwidget.moc | grep 'char.*className' | sed -e 's/.*[^A-Za-z0-9_:]\([A-Za-z0-9_:]*\)::className.*$$/\"\1\",/' | sort | uniq >> libkdevgrepview_la_meta_unload.cpp
	@echo '0};' >> libkdevgrepview_la_meta_unload.cpp
	@echo '#include <kunload.h>' >> libkdevgrepview_la_meta_unload.cpp
	@echo '_UNLOAD(libkdevgrepview_la)' >> libkdevgrepview_la_meta_unload.cpp


#>+ 3
clean-moc-classes:
	-rm -f  libkdevgrepview_la_meta_unload.cpp

#>+ 11
libkdevgrepview_la.all_cpp.cpp: $(srcdir)/Makefile.in $(srcdir)/grepviewfactory.cpp $(srcdir)/grepviewpart.cpp $(srcdir)/grepviewwidget.cpp $(srcdir)/grepdlg.cpp $(srcdir)/grepconfigwidget.cpp $(srcdir)/libkdevgrepview_la_meta_unload.cpp   grepviewpart.moc grepdlg.moc grepviewfactory.moc grepviewwidget.moc grepconfigwidget.moc 
	@echo 'creating libkdevgrepview_la.all_cpp.cpp ...'; \
	rm -f libkdevgrepview_la.all_cpp.files libkdevgrepview_la.all_cpp.final; \
	echo "#define KDE_USE_FINAL 1" >> libkdevgrepview_la.all_cpp.final; \
	for file in grepviewfactory.cpp grepviewpart.cpp grepviewwidget.cpp grepdlg.cpp grepconfigwidget.cpp libkdevgrepview_la_meta_unload.cpp ; do \
	  echo "#include \"$$file\"" >> libkdevgrepview_la.all_cpp.files; \
	  test ! -f $(srcdir)/$$file || egrep '^#pragma +implementation' $(srcdir)/$$file >> libkdevgrepview_la.all_cpp.final; \
	done; \
	cat libkdevgrepview_la.all_cpp.final libkdevgrepview_la.all_cpp.files  > libkdevgrepview_la.all_cpp.cpp; \
	rm -f libkdevgrepview_la.all_cpp.final libkdevgrepview_la.all_cpp.files

#>+ 3
clean-final:
	-rm -f libkdevgrepview_la.all_cpp.cpp

