<!DOCTYPE UI><UI version="3.0" stdsetdef="1">
<class>UIChooser</class>
<widget class="QWidget">
    <property name="name">
        <cstring>UIChooser</cstring>
    </property>
    <property name="geometry">
        <rect>
            <x>0</x>
            <y>0</y>
            <width>467</width>
            <height>448</height>
        </rect>
    </property>
    <property name="caption">
        <string>Form1</string>
    </property>
    <vbox>
        <property name="name">
            <cstring>unnamed</cstring>
        </property>

        <widget class="QButtonGroup">
            <property name="name">
                <cstring>ButtonGroup1</cstring>
            </property>
            <property name="title">
                <string>Major &amp;User-Interface Mode</string>
            </property>
            <vbox>
                <property name="name">
                    <cstring>unnamed</cstring>
                </property>

                <widget class="QRadioButton">
                    <property name="name">
                        <cstring>modeMDI</cstring>
                    </property>
                    <property name="text">
                        <string>&amp;Childframe window mode</string>
                    </property>
                    <property name="whatsThis" stdset="0">
                        <string>All tool views are initially docked to the mainframe.
Editor and browser views will live like toplevel windows within a view area of the mainframe.
A typical example of this user interface mode is MS Visual Studio 6.0.</string>
                    </property>
                </widget>
                <widget class="QRadioButton">
                    <property name="name">
                        <cstring>modeTab</cstring>
                    </property>
                    <property name="text">
                        <string>&amp;Tabbed pages mode</string>
                    </property>
                    <property name="whatsThis" stdset="0">
                        <string>All tool views are initially docked to the mainframe.
Editor and browser views will be stacked in a tab window.
A typical example of this user interface mode is KDEStudio, our friend C++-IDE in the world of KDE.</string>
                    </property>
                </widget>
                <widget class="QRadioButton">
                    <property name="name">
                        <cstring>modeToplevel</cstring>
                    </property>
                    <property name="text">
                        <string>T&amp;oplevel window mode</string>
                    </property>
                    <property name="whatsThis" stdset="0">
                        <string>All editor, browser and tool views will be toplevel windows (directly on desktop).
The main widget contains the menu, toolbars and statusbar, only.
A typical example of this user interface mode is Borland Delphi 6.0.</string>
                    </property>
                </widget>
                <widget class="QRadioButton">
                    <property name="name">
                        <cstring>modeIDEAl</cstring>
                    </property>
                    <property name="text">
                        <string>I&amp;DEAl window mode (alpha state!)</string>
                    </property>
                    <property name="whatsThis" stdset="0">
                        <string>This is a clone of the IDEA user interface, similar to the Tabbed pages mode.</string>
                    </property>
                </widget>
                <widget class="QRadioButton">
                    <property name="name">
                        <cstring>modeIDEA</cstring>
                    </property>
                    <property name="text">
                        <string>&amp;IDEAl mode</string>
                    </property>
                    <property name="whatsThis" stdset="0">
                        <string>This is a clone of the IDEA user interface, similar to the Tabbed pages mode.</string>
                    </property>
                </widget>
            </vbox>
        </widget>
        <widget class="QLabel">
            <property name="name">
                <cstring>TextLabel1</cstring>
            </property>
            <property name="font">
                <font>
                    <italic>1</italic>
                </font>
            </property>
            <property name="margin">
                <number>0</number>
            </property>
            <property name="text">
                <string>Please note that a change to and from IDEAl mode requires a restart of the program.</string>
            </property>
            <property name="alignment">
                <set>WordBreak|AlignVCenter</set>
            </property>
        </widget>
        <spacer>
            <property name="name">
                <cstring>Spacer2</cstring>
            </property>
            <property name="orientation">
                <enum>Vertical</enum>
            </property>
            <property name="sizeType">
                <enum>Expanding</enum>
            </property>
            <property name="sizeHint">
                <size>
                    <width>0</width>
                    <height>20</height>
                </size>
            </property>
        </spacer>
    </vbox>
</widget>
<includes>
    <include location="global" impldecl="in implementation">kdialog.h</include>
</includes>
<layoutdefaults spacing="6" margin="11"/>
<layoutfunctions spacing="KDialog::spacingHint" margin="KDialog::marginHint"/>
</UI>
